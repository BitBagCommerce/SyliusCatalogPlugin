<?xml version="1.0" encoding="utf-8" ?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="bitbag_sylius_catalog_plugin.twig.extension.product_catalog" class="BitBag\SyliusCatalogPlugin\Twig\Extension\RenderProductCatalogExtension" public="false">
            <tag name="twig.extension" />
            <argument type="service" id="templating" />
            <argument type="service" id="bitbag_sylius_catalog_plugin.registry_catalog_rule_checker" />
            <argument type="service" id="sylius.repository.product" />
            <argument type="service" id="bitbag.resolver.catalog.catalog_resource" />
            <argument type="service" id="bitbag_sylius_catalog_plugin.repository.catalog_rule" />

        </service>
    </services>
</container>



